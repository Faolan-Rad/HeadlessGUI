{
  "name": "headlesscore",
  "productName": "Headless Core",
  "version": "v0.1.2",
  "description": "Headless Server for Neos",
  "main": "start.js",
  "scripts": {
    "start": "electron .",
    "package-linux-ia32": "electron-packager . \"Neos Headless Manager\" --overwrite --asar --platform=linux --arch=ia32 --icon=images/icon.ico --prune=true --out=release-builds --version-string.CompanyName=PolyLogiX --version-string.FileDescription=CE --version-string.ProductName=\"Neos Headless Manager\"",
    "package-linux-arm64": "electron-packager . \"Neos Headless Manager\" --overwrite --asar --platform=linux --arch=arm64 --icon=images/icon.ico --prune=true --out=release-builds --version-string.CompanyName=PolyLogiX --version-string.FileDescription=CE --version-string.ProductName=\"Neos Headless Manager\"",
    "package-linux-x64": "electron-packager . \"Neos Headless Manager\" --overwrite --asar --platform=linux --arch=x64 --icon=images/icon.ico --prune=true --out=release-builds --version-string.CompanyName=PolyLogiX --version-string.FileDescription=CE --version-string.ProductName=\"Neos Headless Manager\"",
    "package-linux-armv7l": "electron-packager . \"Neos Headless Manager\" --overwrite --asar --platform=linux --arch=armv7l --icon=images/icon.ico --prune=true --out=release-builds --version-string.CompanyName=PolyLogiX --version-string.FileDescription=CE --version-string.ProductName=\"Neos Headless Manager\"",
    "package-win32-arm64": "electron-packager . \"Neos Headless Manager\" --overwrite --asar --platform=win32 --arch=arm64 --icon=images/icon.ico --prune=true --out=release-builds --version-string.CompanyName=PolyLogiX --version-string.FileDescription=CE --version-string.ProductName=\"Neos Headless Manager\"",
    "package-win32-ia32": "electron-packager . \"Neos Headless Manager\" --overwrite --asar --platform=win32 --arch=ia32 --icon=images/icon.ico --prune=true --out=release-builds --version-string.CompanyName=PolyLogiX --version-string.FileDescription=CE --version-string.ProductName=\"Neos Headless Manager\"",
    "package-win32-x64": "electron-packager . \"Neos Headless Manager\" --overwrite --asar --platform=win32 --arch=x64 --icon=images/icon.ico --prune=true --out=release-builds --version-string.CompanyName=PolyLogiX --version-string.FileDescription=CE --version-string.ProductName=\"Neos Headless Manager\"",
    "package-all": "electron-packager . \"Neos Headless Manager\" --overwrite --asar --all --icon=images/icon.ico --prune=true --out=release-builds --version-string.CompanyName=PolyLogiX --version-string.FileDescription=CE --version-string.ProductName=\"Neos Headless Manager\""
  },
  "author": "Ethan Snow",
  "license": "MIT",
  "dependencies": {
    "electron": "^7.1.1",
    "electron-css-reload": "^1.0.8",
    "electron-store": "^5.1.0",
    "fs-extra": "^8.1.0",
    "node-fetch": "^2.6.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "electron-packager": "^14.1.0"
  }
}
